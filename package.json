{
  "name": "vuetifier",
  "version": "1.0.0",
  "description": "Vuetify.js color and component examples.",
  "author": "reireias",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "DEPLOY_ENV=GH_PAGES nuxt build",
    "start": "nuxt start",
    "start-static": "http-server ./dist",
    "generate": "DEPLOY_ENV=GH_PAGES VUETIFY_VERSION=$(node scripts/version.js) nuxt generate",
    "generate-local": "VUETIFY_VERSION=$(node scripts/version.js) nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/pwa": "^3.2.2",
    "cross-env": "^7.0.3",
    "nuxt": "^2.14.7",
    "vuetify": "^2.3.16"
  },
  "devDependencies": {
    "@mdi/font": "^5.8.55",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/test-utils": "^1.1.1",
    "@yarnpkg/lockfile": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": ">=14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": ">=2.22.1",
    "eslint-plugin-jest": ">=24.1.0",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.2",
    "eslint-plugin-vue": "^7.1.0",
    "http-server": "^0.12.3",
    "jest": "^24.1.0",
    "jest-canvas-mock": "^2.3.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "push-dir": "^0.4.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "typescript": "^4.0.5",
    "vue-jest": "^4.0.0-0"
  }
}
