{
  "name": "vuetifier",
  "version": "1.0.0",
  "description": "Vuetify.js color and component examples.",
  "author": "reireias",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "DEPLOY_ENV=GH_PAGES nuxt build",
    "start": "nuxt start",
    "start-static": "http-server ./dist",
    "generate": "DEPLOY_ENV=GH_PAGES VUETIFY_VERSION=$(node scripts/version.js) nuxt generate",
    "generate-local": "VUETIFY_VERSION=$(node scripts/version.js) nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.6.0",
    "cross-env": "^5.2.0",
    "nuxt": "^2.8.1",
    "vuetify": "^2.0.7"
  },
  "devDependencies": {
    "@mdi/font": "^4.0.96",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/vuetify": "^1.3.3",
    "@yarnpkg/lockfile": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": ">=14.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.15.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "http-server": "^0.11.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.18.2",
    "push-dir": "^0.4.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.5.3"
  }
}
