{
  "name": "vuetifier",
  "version": "1.0.0",
  "description": "Vuetify.js color and component examples.",
  "author": "reireias",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "DEPLOY_ENV=GH_PAGES nuxt build",
    "start": "nuxt start",
    "start-static": "http-server ./dist",
    "generate": "DEPLOY_ENV=GH_PAGES VUETIFY_VERSION=$(node scripts/version.js) nuxt generate",
    "generate-local": "VUETIFY_VERSION=$(node scripts/version.js) nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.6.0",
    "cross-env": "^5.2.0",
    "nuxt": "^2.8.1",
    "vue-color": "^2.7.0",
    "vuetify": "^1.5.5",
    "vuetify-loader": "^1.2.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@yarnpkg/lockfile": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.7.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "http-server": "^0.11.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "push-dir": "^0.4.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
